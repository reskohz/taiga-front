nav.history-tabs(ng-if="vm.activities")
    a.history-tab.active(
        href=""
        title="{{ACTIVITY.TITLE}}"
        translate="ACTIVITY.TITLE"
    )

section.wiki-history(ng-if="vm.activities")
    tg-wiki-history-entry.wiki-history-entry(
        tg-repeat="activity in vm.activities"
        activity="activity"
    )
